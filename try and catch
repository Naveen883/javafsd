package javafsd;

public class MyClass 
{
    public static void main(String args[]) 
    {
        int[] array = new int[7];
        try 
        {
            array[2] = 5;
        }
        catch (ArrayIndexOutOfBoundsException e) 
        {
            System.out.println("It is the collection of object!"); 
        }
        finally 
        {
            System.out.println("This is of size " + array.length);
        }
    }
}
