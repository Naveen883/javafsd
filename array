package assistedpractice;

public class array {

	public static void main(String[] args) {
		int a[]= {11,22,33,44,55,66,77,88,99};
		for(int i=0;i<a.length;i++){
		System.out.println( i +" Elemt in array is : " +a[i]);
		}


		int[][] b = {
		            {4,8,12,16,20},
		            {5,15,25}
		            };
		      System.out.println();
		      System.out.println("Length of Row 1 is;"  +b[0].length);
		      System.out.println("Length of Row 2 is:" +b[1].length);
	}
}
